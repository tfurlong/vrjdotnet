datadir	= @datadir@/Doozer++
prefix	= @prefix@
srcdir	= @srcdir@

install:
	@$(srcdir)/scripts/mkinstalldirs $(datadir)
	cp -r $(srcdir)/config $(datadir)
	cp -r $(srcdir)/mk $(datadir)
	cp -r $(srcdir)/examples $(datadir)
	cp -r $(srcdir)/scripts $(datadir)
